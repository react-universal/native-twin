{
  "name": "universal-labs",
  "version": "0.0.0-development",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind.git"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "workspaces": [
    "packages/*",
    "examples/*",
    "apps/*",
    "e2e/*"
  ],
  "scripts": {
    "build:libs": "nx run-many --target build --projects=\"@universal-labs/**\"",
    "commit": "cz",
    "dev": "nx run-many --target dev --projects=\"@universal-labs/**\" --parallel",
    "docs:dev": "yarn workspace docs-page dev",
    "expo:android": "yarn workspace expo-app android",
    "expo:ios": "yarn workspace expo-app ios",
    "expo:start": "yarn workspace expo-app start",
    "release": "nx run-many --target build --projects=\"@universal-labs/**\" && yarn lerna publish --yes --force-publish",
    "test:e2e": "nx run-many --target e2e --projects=\"@universal-labs/**\"",
    "version": "yarn install && git stage yarn.lock",
    "version-packages": "changeset version && yarn --mode=\"update-lockfile\"",
    "web": "nx run --target dev --project next-app"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@nrwl/cypress": "15.8.5",
    "@nrwl/eslint-plugin-nx": "15.8.5",
    "@nrwl/jest": "15.8.5",
    "@nrwl/js": "15.8.5",
    "@nrwl/linter": "15.8.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "15.8.5",
    "@nrwl/vite": "15.8.5",
    "@nrwl/workspace": "15.8.5",
    "@nxext/vite": "15.1.0",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "~0.25.8",
    "@vitest/ui": "^0.25.8",
    "c8": "^7.12.0",
    "cypress": "^12.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "~20.0.3",
    "nx": "15.8.5",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~4.9.5",
    "vite": "^4.0.1",
    "vite-plugin-dts": "~1.7.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.25.8",
    "vitest-preview": "latest"
  },
  "packageManager": "yarn@1.22.19"
}
