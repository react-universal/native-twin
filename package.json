{
  "name": "universal-labs",
  "version": "0.0.0-development",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind.git"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "workspaces": [
    "packages/*",
    "examples/*",
    "apps/*"
  ],
  "scripts": {
    "build:libs": "npx nx run-many --target=build:libs",
    "build:release": "yarn build:libs && yarn libs:foreach pack && yarn libs:foreach exec \"mkdir _release && tar zxvf package.tgz --directory _release && rm package.tgz\"",
    "dev": " nx watch --projects=stylesheets,primitives,styled,css,twind-adapter -- nx run \\$NX_PROJECT_NAME:build:libs",
    "docs:build": "npx nx run build --project=docusaurus",
    "docs:dev": "npx nx run dev --project=docusaurus",
    "expo:android": "yarn workspace expo-app run android",
    "expo:ios": "yarn workspace expo-app run ios",
    "expo:start": "yarn workspace expo-app run start",
    "format": "prettier --write packages apps examples \"**/*.{js,ts,tsx,md}\"",
    "libs:foreach": "yarn workspaces foreach --no-private --verbose",
    "lint": "npx nx run-many --target=lint",
    "release": "yarn build:release && changeset publish",
    "test": "npx nx run-many --target=test",
    "web": "npx nx dev start --project=next-app"
  },
  "resolutions": {
    "@twind/core@1.1.3": "patch:@twind/core@npm%3A1.1.3#./.yarn/patches/@twind-core-npm-1.1.3-62b4eeba21.patch",
    "vitest-react-native@^0.1.2": "patch:vitest-react-native@npm%3A0.1.2#./.yarn/patches/vitest-react-native-npm-0.1.2-0858752826.patch"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@nrwl/devkit": "15.8.7",
    "@nrwl/eslint-plugin-nx": "15.8.7",
    "@nrwl/js": "15.9.2",
    "@nrwl/linter": "15.8.7",
    "@nrwl/nx-cloud": "latest",
    "@rollup/plugin-terser": "^0.4.1",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "@vitest/coverage-istanbul": "^0.31.1",
    "@vitest/ui": "^0.31.1",
    "concurrently": "^7.6.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "expo-module-scripts": "^3.0.11",
    "happy-dom": "latest",
    "jsdom": "latest",
    "nx": "15.8.7",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tslib": "^2.6.0",
    "typescript": "^5.1.3",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "^0.31.1",
    "vitest-react-native": "^0.1.2"
  },
  "packageManager": "yarn@3.4.1"
}
