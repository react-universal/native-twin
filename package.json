{
  "name": "universal-labs",
  "version": "0.0.0-development",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind.git"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "workspaces": [
    "packages/*",
    "examples/*",
    "apps/*"
  ],
  "scripts": {
    "build:libs": "yarn nx run-many --target=build:libs",
    "build:release": "yarn build:libs && yarn libs:foreach pack && yarn libs:foreach exec \"mkdir _release && tar zxvf package.tgz --directory _release && rm package.tgz\"",
    "expo:android": "yarn workspace expo-app run android",
    "expo:ios": "yarn workspace expo-app run ios",
    "expo:start": "yarn workspace expo-app run start",
    "format": "prettier --write packages apps examples \"**/*.{js,ts,tsx,md}\"",
    "libs:dev": "nx watch --projects=styled,css,twind-adapter -- nx run \\$NX_PROJECT_NAME:build:libs",
    "libs:foreach": "yarn workspaces foreach --no-private --verbose",
    "lint": "npx nx run-many --target=lint",
    "plugin:dev": "nx run-many --target vscode:dev --parallel --output-style=dynamic",
    "release": "yarn build:release && changeset publish",
    "site:dev": "npx nx run dev --project=site",
    "test": "npx nx run-many --target=test"
  },
  "resolutions": {
    "@twind/core@1.1.3": "patch:@twind/core@npm%3A1.1.3#./.yarn/patches/@twind-core-npm-1.1.3-62b4eeba21.patch",
    "vitest-react-native@^0.1.2": "patch:vitest-react-native@npm%3A0.1.2#./.yarn/patches/vitest-react-native-npm-0.1.2-0858752826.patch"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@nx/devkit": "16.6.0",
    "@nx/eslint-plugin": "16.6.0",
    "@nx/js": "16.6.0",
    "@nx/linter": "16.6.0",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/chai": "4.3.5",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-istanbul": "^0.31.1",
    "@vitest/ui": "^0.31.1",
    "@vscode/test-electron": "^2.3.2",
    "chai": "4.3.7",
    "concurrently": "^7.6.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "glob": "^8.1.0",
    "happy-dom": "latest",
    "jsdom": "latest",
    "mocha": "^10.2.0",
    "nx": "16.6.0",
    "nx-cloud": "16.2.0",
    "prettier": "^2.8.3",
    "react-test-renderer": "18.2.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.3",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-multiple": "0.2.0",
    "vitest": "^0.31.1",
    "vitest-react-native": "^0.1.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.1"
}
