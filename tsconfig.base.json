{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "moduleResolution": "Node",
    "target": "ESNext",
    "strict": true,
    "strictNullChecks": true,
    "module": "ESNext",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {}
  },
  "exclude": [
    "node_modules",
    "tmp",
    "**/dist/**",
    "**/.expo/**/*",
    "dist",
    "**/.next/**/*"
  ]
}
