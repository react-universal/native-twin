{
  "name": "@native-twin/compiler",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/esm/index.js",
      "require": "./build/index.js",
      "default": "./build/esm/index.js"
    },
    "./node": {
      "types": "./build/node/index.d.ts",
      "import": "./build/esm/node/index.js",
      "require": "./build/node/index.js",
      "default": "./build/esm/node/index.js"
    },
    "./metro": {
      "types": "./build/node/metro/index.d.ts",
      "import": "./build/esm/node/metro/index.js",
      "require": "./build/node/metro/index.js",
      "default": "./build/esm/node/metro/index.js"
    },
    "./babel": {
      "types": "./build/node/babel/index.d.ts",
      "import": "./build/esm/node/babel/index.js",
      "require": "./build/node/babel/index.js",
      "default": "./build/esm/node/babel/index.js"
    },
    "./metro.babel.transformer": {
      "types": "./build/node/metro/transformers/metro.babel.transformer.js",
      "import": "./build/esm/node/metro/transformers/metro.babel.transformer.js",
      "require": "./build/node/metro/transformers/metro.babel.transformer.js",
      "default": "./build/esm/node/metro/transformers/metro.babel.transformer.js"
    },
    "./metro.transformer": {
      "types": "./build/node/metro/transformers/metro.transformer.js",
      "import": "./build/esm/node/metro/transformers/metro.transformer.js",
      "require": "./build/node/metro/transformers/metro.transformer.js",
      "default": "./build/esm/node/metro/transformers/metro.transformer.js"
    }
  },
  "main": "./build/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "twin build",
    "build:watch": "twin build -w",
    "clean": "rm -rf build",
    "gen:jsdocs": "jsdoc -c jsdoc.json",
    "test": "jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@native-twin/css": "workspace:*"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@effect/language-service": "^0.2.0",
    "@native-twin/cli": "workspace:*",
    "@types/babel__helper-module-imports": "^7.18.3",
    "@types/js-beautify": "^1",
    "babel-plugin-tester": "^11.0.4",
    "js-beautify": "^1.15.1",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "optionalDependencies": {
    "@native-twin/cli": "workspace:*",
    "better-docs": "*",
    "jest": "*"
  }
}
