{
  "extends": "../../.eslintrc.json",
  "ignorePatterns": [
    "!**/*",
    "build"
  ],
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "extends": [
        "plugin:@nx/typescript"
      ],
      "rules": {
        "no-empty": "off",
        "require-yield": "off"
