{
  "name": "@universal-labs/core",
  "version": "1.4.16",
  "description": "Tailwindcss Inline StyleSheet generator for react native",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind-plugin",
    "react-native-web",
    "universal-labs",
    "react-native",
    "react"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/native-tailwind"
  },
  "exports": {
    ".": {
      "import": "./build/index.es.js",
      "require": "./build/index.cjs.js",
      "browser": "./build/index.cjs.js",
      "types": "./index.d.ts"
    },
    "./tailwind/preset": {
      "import": "./tailwind/preset.js",
      "require": "./tailwind/preset.js"
    }
  },
  "main": "./build/index.cjs.js",
  "module": "./build/index.es.js",
  "types": "./index.d.ts",
  "files": [
    "build",
    "index.d.ts",
    "tailwind",
    "types.d.ts",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "vite build && yarn build:tailwind",
    "build:tailwind": "vite build -c vite.tailwind.config.ts",
    "dev": "yarn vite build -w",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "postcss": "8.4.21",
    "postcss-color-rgb": "^2.0.0",
    "postcss-css-variables": "^0.18.0",
    "postcss-js": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/postcss-css-variables": "0.18.0",
    "@types/postcss-js": "^4.0.0",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/tailwindcss": "3.1.0",
    "tailwindcss": "3.2.4",
    "tsx": "^3.12.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  },
  "gitHead": "4c26121d296e38153faa7dd196229db1a71e075d"
}
