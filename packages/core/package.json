{
  "name": "@universal-labs/core",
  "version": "1.10.2",
  "description": "Tailwindcss Inline StyleSheet generator for react native",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind-plugin",
    "react-native-web",
    "universal-labs",
    "react-native",
    "react"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/native-tailwind"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs.js",
      "require": "./dist/index.js",
      "browser": "./dist/cdn.min.js",
      "types": "./tailwind.d.ts"
    },
    "./tailwind/preset": {
      "import": "./tailwind/preset.js",
      "require": "./tailwind/preset.js"
    }
  },
  "main": "./dist/index.mjs.js",
  "unpkg": "./dist/cdn.min.js",
  "module": "./dist/index.mjs.js",
  "types": "./tailwind.d.ts",
  "files": [
    "build",
    "index.d.ts",
    "tailwind",
    "tailwind.d.ts",
    "types.d.ts",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:esbuild": "tsx src/build.ts",
    "build:libs": "npx vite build && yarn build:tailwind",
    "build:tailwind": "npx vite build -c vite.tailwind.config.ts",
    "dev": "concurrently \"npx tsx src/build.ts -w\" \"npx vite build -c vite.tailwind.config.ts -w\"",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "postcss-css-variables": "^0.18.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/postcss-css-variables": "0.18.0",
    "@types/tailwindcss": "3.1.0",
    "tsx": "3.12.6"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  },
  "gitHead": "4c26121d296e38153faa7dd196229db1a71e075d"
}
