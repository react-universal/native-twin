{
  "name": "@universal-labs/core",
  "version": "1.4.5",
  "description": "Tailwindcss Inline StyleSheet generator for react native",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind-plugin",
    "react-native",
    "react"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/native-tailwind"
  },
  "exports": {
    ".": {
      "import": "./build/index.es.js",
      "require": "./build/index.cjs.js",
      "browser": "./build/index.umd.js",
      "types": "./index.d.ts"
    },
    "./tailwind/plugin": {
      "import": "./build/tailwind-plugin.cjs",
      "require": "./build/tailwind-plugin.cjs",
      "types": "./src/tailwind/plugin/tailwind-plugin.ts"
    },
    "./tailwind/preset": {
      "import": "./tailwind/preset.js",
      "require": "./tailwind/preset.js"
    }
  },
  "main": "./build/index.cjs.js",
  "unpkg": "./build/index.iife.js",
  "module": "./build/index.es.js",
  "types": "./index.d.ts",
  "files": [
    "src",
    "build",
    "index.d.ts",
    "tailwind",
    "types.d.ts",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "vite build && yarn build:tailwind",
    "build:tailwind": "vite build -c vite.tailwind.config.ts",
    "dev": "yarn vite build -w",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "prepare": "yarn build",
    "test:ui": "vitest --ui"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "postcss": "8.4.21",
    "postcss-color-rgb": "^2.0.0",
    "postcss-css-variables": "^0.18.0",
    "postcss-js": "^4.0.0",
    "postcss-rem-to-pixel": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/postcss-css-variables": "0.18.0",
    "@types/postcss-js": "^4.0.0",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/tailwindcss": "3.1.0",
    "tailwindcss": "3.2.4",
    "vite": "^4.1.1",
    "vitest": "^0.28.3"
  },
  "peerDependencies": {
    "postcss": ">= 8.0.0",
    "tailwindcss": ">= 3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "gitHead": "a5c05d5ee9bc016aa01fc3a7cc03bddd46d1e634"
}
