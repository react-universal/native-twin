{
  "name": "@universal-labs/core",
  "version": "3.0.0",
  "description": "Tailwindcss Inline StyleSheet generator for react native",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind-plugin",
    "react-native-web",
    "universal-labs",
    "react-native",
    "react"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/native-tailwind"
  },
  "exports": {
    ".": {
      "import": "./build/index.mjs.js",
      "require": "./build/index.js",
      "browser": "./build/cdn.min.js",
      "types": "./tailwind.d.ts"
    },
    "./tailwind/preset": {
      "import": "./tailwind/preset.js",
      "require": "./tailwind/preset.js"
    }
  },
  "main": "./build/index.js",
  "unpkg": "./build/cdn.min.js",
  "module": "./build/index.mjs.js",
  "types": "./tailwind.d.ts",
  "files": [
    "build",
    "index.d.ts",
    "tailwind",
    "tailwind.d.ts",
    "types.d.ts",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:esbuild": "tsx src/build.ts",
    "build:libs:tailwind": "npx tsx src/build.ts && yarn build:tailwind",
    "build:tailwind": "npx vite build -c vite.tailwind.config.ts",
    "dev:tailwind": "concurrently \"npx tsx watch src/build.ts\" \"yarn build:tailwind -w\"",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/css-tree": "2.3.1",
    "@types/postcss-css-variables": "0.18.0",
    "@types/tailwindcss": "3.1.0",
    "css-tree": "2.3.1",
    "postcss-css-variables": "^0.18.0",
    "tailwindcss": "^3.2.4",
    "tsx": "3.12.6"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  },
  "gitHead": "4c26121d296e38153faa7dd196229db1a71e075d"
}
