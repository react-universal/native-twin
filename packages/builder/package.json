{
  "name": "@native-twin/cli",
  "version": "2.3.0",
  "description": "Native Twin packages bundler",
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "bugs": {
    "url": "https://github.com/react-universal/native-twin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin",
    "directory": "packages/builder"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "sideEffects": false,
  "type": "module",
  "bin": {
    "twin": "./build/twin-cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node esbuild.mjs",
    "build:watch": "tsc -p tsconfig.build.json -w",
    "lint": "eslint"
  },
  "dependencies": {
    "@effect/cli": "^0.42.3",
    "@effect/platform": "^0.64.1",
    "@effect/platform-node": "^0.59.1",
    "effect": "^3.7.3",
    "esbuild": "0.23.1",
    "fs-extra": "11.2.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.25.6",
    "@chialab/esbuild-rna": "^0.18.2",
    "@types/fs-extra": "11.0.4",
    "minimist": "^1.2.8",
    "sucrase": "^3.35.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  },
  "peer": {
    "@babel/generator": "*",
    "@babel/parser": "*",
    "@babel/traverse": "*"
  }
