{
  "name": "@universal-labs/styled",
  "version": "1.4.18",
  "description": "Styled components for css in react native",
  "keywords": [
    "react-native",
    "tailwindcss",
    "postcss",
    "tailwind-ui",
    "react-native-web",
    "universal-apps"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/styled"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./build/index.es.js",
      "require": "./build/index.umd.js"
    },
    "./tailwind": {
      "import": "./build/tailwind/index.cjs.js",
      "require": "./build/tailwind/index.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./build/index.umd.js",
  "module": "./build/index.es.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && yarn build:native && npx tsc -p tsconfig.build.json",
    "build:native": "vite build -c vite.native.config.ts",
    "dev": "yarn build",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@universal-labs/stylesheets": "workspace:*",
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/react-is": "^17.0.3",
    "@types/react-native": "0.70.7"
  },
  "peerDependencies": {
    "react-native-web": ">= 0.18.10"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  },
  "gitHead": "4c26121d296e38153faa7dd196229db1a71e075d",
  "react-native": "./build/index.native.es.js"
}
