{
  "name": "@universal-labs/native-tailwind-nextjs",
  "version": "5.0.1",
  "private": true,
  "description": "React Native Web Tailwindcss for nextjs",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind-native",
    "react-native-web",
    "universal-labs",
    "react-native",
    "react",
    "nextjs"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/native-tailwind-nextjs"
  },
  "sideEffects": false,
  "exports": {
    "./app": {
      "types": "./build/app.d.ts",
      "module": "./build/app.es.js",
      "import": "./build/app.es.js",
      "node": {
        "import": "./build/app.es.js",
        "require": "./build/app.cjs.js"
      },
      "default": "./build/app.cjs.js"
    },
    "./document": {
      "types": "./build/document.d.ts",
      "module": "./build/document.es.js",
      "node": {
        "import": "./build/document.es.js",
        "require": "./build/document.cjs.js"
      },
      "default": "./build/document.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "index.d.ts",
    "tailwind",
    "tailwind.d.ts",
    "types.d.ts",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:libs": "vite build",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@universal-labs/css": "workspace:*",
    "@universal-labs/native-tailwind": "workspace:*"
  },
  "peerDependencies": {
    "next": ">=13.0",
    "react": ">=18.0",
    "react-native": ">=0.71.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}
