{
  "name": "@universal-labs/native-twin-nextjs",
  "version": "5.0.1",
  "private": true,
  "description": "React Native Web Tailwindcss for nextjs",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind-native",
    "react-native-web",
    "universal-labs",
    "react-native",
    "react",
    "nextjs"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/native-tailwind-nextjs"
  },
  "sideEffects": false,
  "exports": {
    "./_app": {
      "types": "./build/_app.d.ts",
      "module": "./build/_app.es.js",
      "import": "./build/_app.es.js",
      "node": {
        "import": "./build/_app.es.js",
        "require": "./build/_app.cjs.js"
      },
      "default": "./build/_app.cjs.js"
    },
    "./_document": {
      "types": "./build/_document.d.ts",
      "module": "./build/_document.es.js",
      "node": {
        "import": "./build/_document.es.js",
        "require": "./build/_document.cjs.js"
      },
      "default": "./build/_document.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "index.d.ts",
    "tailwind",
    "tailwind.d.ts",
    "types.d.ts",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@universal-labs/native-twin": "workspace:*"
  },
  "peerDependencies": {
    "next": ">=13.0",
    "react": ">=18.0",
    "react-native": ">=0.71.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}
