{
  "name": "@react-universal/tailwind",
  "version": "0.0.9",
  "description": "Tailwindcss Inline CSS",
  "keywords": [
    "tailwind",
    "tailwindcss"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-universal/tailwind.git",
    "directory": "packages/tailwind"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs.js",
      "require": "./dist/index.js",
      "browser": "./dist/cdn.min.js",
      "types": "./index.d.ts"
    }
  },
  "main": "dist/index.js",
  "unpkg": "dist/cdn.min.js",
  "module": "dist/index.mjs.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsx src/build.ts",
    "test:ui": "vitest --ui"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "postcss": "8.4.21",
    "postcss-css-variables": "0.18.0",
    "postcss-rem-to-pixel": "^4.1.2",
    "tailwindcss": "3.2.4"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/postcss-css-variables": "0.18.0",
    "@types/react": "^18.0.27",
    "@types/tailwindcss": "3.1.0",
    "esbuild": "0.15.18",
    "tsx": "3.12.2",
    "typescript": "4.9.4",
    "vite": "^4.1.1",
    "vitest": "^0.28.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "react-native": "./src/index.ts"
}
