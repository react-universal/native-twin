{
  "name": "primitives-nativewind.view",
  "sourceRoot": "packages/primitives/nativewind.view/src",
  "projectType": "library",
  "tags": [
    "nativewind",
    "primitives"
  ],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/primitives/nativewind.view/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/primitives/nativewind.view",
        "tsConfig": "packages/primitives/nativewind.view/tsconfig.lib.json",
        "project": "packages/primitives/nativewind.view/package.json",
        "entryFile": "packages/primitives/nativewind.view/src/index.ts",
        "external": [
          "react/jsx-runtime"
        ],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "packages/primitives/nativewind.view/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/primitives/nativewind.view/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
