{
  "name": "@universal-labs/primitives",
  "version": "1.4.19",
  "description": "React Native primitives in styled version",
  "keywords": [
    "react-native",
    "tailwindcss",
    "postcss",
    "tailwind-ui",
    "react-native-web",
    "react-native-tailwindcss",
    "react-native-tailwind-components",
    "universal-apps"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/primitives"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "env.d.ts",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "expo-module build -p tsconfig.build.json",
    "build:native": "npx vite build -c vite.native.config.ts",
    "build:vite": "npx vite build && yarn build:native && npx tsc -p tsconfig.build.json",
    "dev": "yarn vite build -w --force",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:ui": "yarn vitest --ui"
  },
  "devDependencies": {
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/react-native": "0.70.7",
    "@universal-labs/styled": "workspace:*"
  },
  "peerDependencies": {
    "react-native-web": ">= 0.18.10"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  },
  "gitHead": "4c26121d296e38153faa7dd196229db1a71e075d",
  "react-native": "./build/index.native.cjs.js"
}
