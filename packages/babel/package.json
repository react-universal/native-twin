{
  "name": "@native-twin/babel",
  "version": "6.4.0",
  "description": "Native Twin babel plugin",
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "bugs": {
    "url": "https://github.com/react-universal/native-twin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin",
    "directory": "packages/babel"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "require": "./build/cjs/index.cjs",
      "import": "./build/esm/index.mjs"
    },
    "./babel": {
      "require": "./babel.js",
      "import": "./babel.js",
      "default": "./babel.js"
    },
    "./plugin": {
      "require": "./plugin.js",
      "import": "./plugin.js",
      "default": "./plugin.js"
    },
    "./jsx-babel": {
      "types": "./build/types/jsx/index.d.ts",
      "require": "./build/cjs/jsx/index.cjs",
      "import": "./build/esm/jsx/index.mjs"
    },
    "./jsx-babel/constants": {
      "types": "./build/types/constants/index.d.ts",
      "require": "./build/cjs/constants/index.cjs",
      "import": "./build/esm/constants/index.mjs"
    },
    "./jsx-babel/models": {
      "types": "./build/types/jsx/models/index.d.ts",
      "require": "./build/cjs/jsx/models/index.cjs",
      "import": "./build/esm/jsx/models/index.mjs"
    },
    "./jsx-babel/plugin": {
      "types": "./build/types/jsx/plugin/index.d.ts",
      "require": "./build/cjs/jsx/plugin/index.cjs",
      "import": "./build/esm/jsx/plugin/index.mjs"
    },
    "./jsx-babel/services": {
      "types": "./build/types/jsx/services/index.d.ts",
      "require": "./build/cjs/jsx/services/index.cjs",
      "import": "./build/esm/jsx/services/index.mjs"
    },
    "./jsx-babel/twin": {
      "types": "./build/types/jsx/twin/index.d.ts",
      "require": "./build/cjs/jsx/twin/index.cjs",
      "import": "./build/esm/jsx/twin/index.mjs"
    }
  },
  "main": "./build/cjs/index",
  "module": "./build/esm/index",
  "types": "./build/types/index.d.ts",
  "scripts": {
    "build": "yarn twin build",
    "build:watch": "tsc -p tsconfig.build.json -w",
    "clean": "rm -rf build",
    "dev": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/helper-module-imports": "^7.22.15",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@native-twin/core": "workspace:*",
    "@native-twin/css": "workspace:*",
    "@native-twin/helpers": "workspace:*",
    "@native-twin/preset-tailwind": "workspace:*",
    "effect": "^3.7.3"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@native-twin/cli": "workspace:*",
    "@types/babel__helper-module-imports": "^7.18.3",
    "babel-plugin-tester": "^11.0.4",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "peerDependencies": {
    "react-native-reanimated": "*"
  },
  "optionalDependencies": {
    "jest": "*"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  }
}
