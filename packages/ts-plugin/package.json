{
  "name": "@native-twin/native-twin-ts-plugin",
  "version": "5.0.5",
  "description": "TypeScript plugin that adds IntelliSense Native Tailwind",
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "main": "build/index.js",
  "scripts": {
    "build": "node ./build.js --production",
    "clean": "rm -rf build",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "vscode:dev": "node ./build.js --watch"
  },
  "dependencies": {
    "@ctrl/tinycolor": "3.6.0",
    "@native-twin/css": "workspace:*",
    "@native-twin/helpers": "workspace:*",
    "@native-twin/native-twin": "workspace:*",
    "@native-twin/preset-tailwind": "workspace:*",
    "@vscode/emmet-helper": "2.9.2",
    "cssbeautify": "0.3.1",
    "match-sorter": "6.3.1",
    "quick-lru": "6.1.1",
    "showdown": "2.1.0",
    "typescript-template-language-service-decorator": "2.3.2",
    "vscode-css-languageservice": "6.2.6",
    "vscode-languageserver-textdocument": "1.0.8"
  },
  "devDependencies": {
    "@types/cssbeautify": "0.3.2",
    "@types/showdown": "2.0.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  }
}
