{
  "name": "@native-twin/helpers",
  "version": "6.4.0",
  "description": "Helpers for common utilities",
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "bugs": {
    "url": "https://github.com/react-universal/native-twin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin",
    "directory": "packages/helpers"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs",
      "types": "./build/types/index.d.ts"
    },
    "./KeyMap": {
      "import": "./build/esm/KeyMap/index.mjs",
      "require": "./build/cjs/KeyMap/index.cjs",
      "types": "./build/types/KeyMap/index.d.ts"
    },
    "./tree": {
      "import": "./build/esm/tree/index.mjs",
      "require": "./build/cjs/tree/index.cjs",
      "types": "./build/types/tree/index.d.ts"
    },
    "./server": {
      "import": "./build/esm/server/index.mjs",
      "require": "./build/cjs/server/index.cjs",
      "types": "./build/types/server/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./build/cjs/index.cjs",
  "module": "./build/esm/index.mjs",
  "types": "./build/types/index.d.ts",
  "files": [
    "build",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "yarn twin build",
    "build:twin": "twin-build",
    "build:types": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json -w",
    "clean": "rm -rf build",
    "dev": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "devDependencies": {
    "@native-twin/cli": "workspace:*"
  },
  "optionalDependencies": {
    "jest": "*"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  },
  "gitHead": "4c26121d296e38153faa7dd196229db1a71e075d"
}
