{
  "name": "@universal-labs/stylesheets",
  "version": "1.6.2",
  "description": "Stylesheets manager",
  "keywords": [
    "react-native",
    "tailwindcss",
    "postcss",
    "tailwind-ui",
    "react-native-web",
    "universal-apps"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/stylesheets"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./react-native": {
      "import": "./react-native/index.js",
      "require": "./react-native/index.js",
      "types": "./react-native/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "env.d.ts",
    "react-native",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build:libs": "expo-module build -p tsconfig.build.json",
    "clean": "expo-module clean",
    "dev": "expo-module build -p tsconfig.build.json -w",
    "expo-module": "expo-module",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "prepublishOnly": "expo-module prepublishOnly",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@universal-labs/core": "workspace:*",
    "css-to-react-native": "^3.1.0",
    "immer": "^9.0.19",
    "react-native-uuid": "^2.0.1",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@types/css-to-react-native": "^3.0.0",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/react-native": "^0.71.5",
    "@types/use-sync-external-store": "^0.0.3",
    "vitest": "^0.28.3"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  },
  "gitHead": "4c26121d296e38153faa7dd196229db1a71e075d"
}
