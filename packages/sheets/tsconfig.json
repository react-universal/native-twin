{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["ESNext"],
    "declarationMap": false,
    "noImplicitAny": true,
    "emitDeclarationOnly": true,
