{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "error",
    "noImplicitAny": false,
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "stripInternal": true,
    "target": "esnext"
  },
  "include": ["src"]
}
