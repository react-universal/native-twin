{
  "name": "@native-twin/jsx",
  "version": "7.0.0-next.1",
  "description": "Native Twin JSX runtime",
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "bugs": {
    "url": "https://github.com/react-universal/native-twin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin",
    "directory": "packages/jsx"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/esm",
      "require": "./build"
    },
    "./jsx-dev-runtime": {
      "types": "./build/jsx-dev-runtime.d.ts",
      "import": "./build/esm/jsx-dev-runtime",
      "require": "./build/jsx-dev-runtime"
    },
    "./jsx-runtime": {
      "types": "./build/jsx-runtime.d.ts",
      "import": "./build/esm/jsx-runtime",
      "require": "./build/jsx-runtime"
    },
    "./sheet": {
      "types": "./build/sheet/index.d.ts",
      "import": "./build/esm/sheet",
      "require": "./build/sheet"
    },
    "./testing-library": {
      "types": "./build/testing-library/index.d.ts",
      "import": "./build/esm/testing-library",
      "require": "./build/testing-library"
    },
    "./testing-library/setup": {
      "types": "./build/testing-library/setup.d.ts",
      "import": "./build/esm/testing-library/setup.js",
      "require": "./build/testing-library/setup.js"
    },
    "./testing-library/setupAfterEnv": {
      "types": "./build/testing-library/setupAfterEnv.d.ts",
      "import": "./build/esm/testing-library/setupAfterEnv.js",
      "require": "./build/testing-library/setupAfterEnv.js"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./rn-svg": {
      "types": "./rn-svg.d.ts"
    }
  },
  "main": "./build",
  "module": "./build/esm",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "jsx-dev-runtime",
    "jsx-runtime",
    "LICENSE",
    "sheet",
    "src",
    "testing-library",
    "rn-svg.d.ts",
    "third-party-declarations.d.ts",
    "types.d.ts",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "twin build",
    "build:watch": "twin build -w",
    "clean": "rm -rf build",
    "dev": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand --silent false --watch",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@native-twin/core": "workspace:*",
    "@native-twin/css": "workspace:*",
    "@native-twin/helpers": "workspace:*",
    "@native-twin/preset-tailwind": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": "*"
  },
  "optionalDependencies": {
    "@native-twin/cli": "workspace:*",
    "jest": "*"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  }
}
